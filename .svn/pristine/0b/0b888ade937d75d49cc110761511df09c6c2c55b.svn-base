<div class="top-box">
    <div class="btc-ticker">
        <span id="ask_price" class="value">306.49 € / 1 BTC</span>
    </div>
    <div class="send">
        <form class="form autoform" id="form-withdraw" action="#" method="post">
            Send <input type="text" name="amount" id="input-amount-multisig" class="amount" placeholder=".25"> btc to <input type="text" name="btc_address" id="input-btc-address" class="address" placeholder="1JuBoovp6uNVZETadvBzCTTDCaNJCKcz16">
            <button id="btc-button-submit" name="send" type="submit" class="send-btc">Go</button>
        </form>
    </div>
    <div class="mobile-send">
        <button class="open-mobile-send">Send btc</button>
        <div class="mobile-send-form"><form class="form autoform" id="form-withdraw" action="#" method="post">
            <span class="mobile-send-amount">Amount: <input type="text" name="amount" id="input-amount-mobile" class="amount" placeholder=".25"></span>
            To: <input type="text" name="btc_address" id="input-btc-address" class="address" placeholder="1JuBoovp6uNVZETadvBzCTTDCaNJCKcz16">
            </form>
            <div class = "mobile-submit-buttons">
            <button id="btc-button-submit-mobile" name="send" type="submit" class="send-btc">Send</button>
            <button id="btc-button-cancel">Cancel</button>
            </div>
        </div>
    </div>
    <div class ="top-buttons">
        <div class="message-icon">
            <a href="messages.html"><img src="img/messages.png" class="messages" id="messages" alt="messages"></a>
            <span class="unread-messages">3</span>
        </div>
        <div class="settings-icon">
            <a href="settings.html"><img src="img/settings.png" class="settings" id="settings" alt="settings"></a>
        </div>
    </div>
    <div class="show-conversion">
        <span class="converted">≈ 0.00 €</span>
    </div>
    <section id="system-messages-send" class="block block-system-messages"></section>
</div>

