<section id="system-messages-login" class="block block-system-messages"></section>
    
  <section>
    <div class="login">
        <h1 class="field title">
            Sign in
        </h1><a name="login"></a>
        <form class="form autoform" id="form-sign-in" action="/login?ajax_content=true#login" method="post">
            <div class="field form-field e-mail">
                <input type="text" name="email" id="input-e-mail" value="" class="input" autofocus="autofocus" placeholder=" E-mail" />
            </div>
            <div class="field form-field password">
                <input type="password" name="password" id="input-password" value="" class="input" placeholder=" Password" />
            </div>
            <div class="field form-field submit">
                <input class="button" id="input-submit" name="login" type="submit" value="Sign in" />
            </div>
            <div class="field recover-password">
                <a href="/forgot_password">I forgot my password</a>
            </div><input type="hidden" name="login" value="true" />
        </form>
    </div>
</section>

