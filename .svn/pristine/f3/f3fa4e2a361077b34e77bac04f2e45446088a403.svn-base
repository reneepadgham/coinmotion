<body id="messagespage">

<header>

    <div class="main-nav">
        <div id="mobile-header">
            <a id="responsive-menu-button" href="#sidr-main"><img src="img/menu.png" alt="menu"></a>
        </div>
        <nav>
            <div class="logo-area"><img src="img/logowhite.png" alt="logo" class="logo"></div>
